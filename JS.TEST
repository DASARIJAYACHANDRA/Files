7.const nestedArray = [1, [2, [3, [4, 5]]]];
function flattenArray(arr) {
  while (arr.some(Array.isArray)) {
    arr = [].concat.apply([], arr);
  }
  return arr;
}
const flattenedArray = flattenArray(nestedArray);

console.log(flattenedArray);

